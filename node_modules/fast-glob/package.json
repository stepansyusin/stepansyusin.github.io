    "clean": "rimraf out",
    "compile": "tsc",
    "lint": "tslint \"src/**/*.ts\" -p . -t stylish",
    "smoke": "mocha \"out/**/*.smoke.js\" -s 0",
    "smoke:async": "mocha \"out/**/*.smoke.js\" -s 0 --grep \"\\(async\\)\"",
    "smoke:stream": "mocha \"out/**/*.smoke.js\" -s 0 --grep \"\\(stream\\)\"",
    "smoke:sync": "mocha \"out/**/*.smoke.js\" -s 0 --grep \"\\(sync\\)\"",
